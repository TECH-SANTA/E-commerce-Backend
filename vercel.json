{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "rewrite": [
        {
            "src": "/(.*)",
            "destination": "/server.js"
        }
    ],
    "headers": [
        {
            "source": "/*",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' blob: data:;"
                }
            ]
        }
    ]
}
